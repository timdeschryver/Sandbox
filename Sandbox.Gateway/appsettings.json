{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"AllowedHosts": "*",
	"ReverseProxy": {
		"Routes": {
			"apiservice": {
				"ClusterId": "apiservice",
				"RateLimiterPolicy": "user-or-ip",
				"Match": {
					"Path": "/api/{**catch-all}"
				},
				"AuthorizationPolicy": "Default",
				"Transforms": [
					{
						"PathRemovePrefix": "/api"
					}
				]
			},
			"otelcollector": {
				"ClusterId": "otelcollector",
				"Match": {
					"Path": "/v1/traces"
				}
			},
			"angularfrontend": {
				"ClusterId": "angularfrontend",
				"Match": {
					"Path": "/{**catch-all}"
				}
			}
		},
		"Clusters": {
			"apiservice": {
				"Destinations": {
					"apiservice": {
						"Address": "http://apiservice"
					}
				}
			},
			"otelcollector": {
				"Destinations": {
					"otelcollector": {
						"Address": "http://otelcollector:4318"
					}
				}
			},
			"angularfrontend": {
				"Destinations": {
					"angularfrontend": {
						"Address": "http://angular-frontend"
					}
				}
			}
		}
	}
}
