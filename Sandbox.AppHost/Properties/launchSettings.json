{
	"$schema": "https://json.schemastore.org/launchsettings.json",
	"profiles": {
		"https": {
			"commandName": "Project",
			"dotnetRunMessages": true,
			"launchBrowser": true,
			"applicationUrl": "https://sandbox.dev.localhost:16258;http://sandbox.dev.localhost:15258",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development",
				"DOTNET_ENVIRONMENT": "Development",
				"ASPIRE_DASHBOARD_OTLP_ENDPOINT_URL": "https://localhost:19280",
				"ASPIRE_DASHBOARD_MCP_ENDPOINT_URL": "http://localhost:15258",
				// Required to use the MCP server - this needs to be over http in VS Code
				"ASPIRE_ALLOW_UNSECURED_TRANSPORT": "true"
			}
		},
		"http": {
			"commandName": "Project",
			"dotnetRunMessages": true,
			"launchBrowser": true,
			"applicationUrl": "http://sandbox.dev.localhost:15258",
			"environmentVariables": {
				"ASPNETCORE_ENVIRONMENT": "Development",
				"DOTNET_ENVIRONMENT": "Development",
				"ASPIRE_DASHBOARD_OTLP_ENDPOINT_URL": "http://localhost:19093",
				"ASPIRE_DASHBOARD_MCP_ENDPOINT_URL": "http://localhost:18263",
				"ASPIRE_ALLOW_UNSECURED_TRANSPORT": "true"
			}
		}
	}
}
